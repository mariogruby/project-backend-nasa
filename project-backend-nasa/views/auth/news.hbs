 <h2>NEWS</h2>
    <div class="container-fluid d-flex flex-wrap">

    {{#each news}}
    <div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <div class="col">
    <div class="p-3">
    <div class="card" style="width: 21rem;">
        <div class="card-body">
        <h6 class="card-title">{{title}}</h6>
         <a href="/news/{{date}}"><img src={{url}} class="card-img-top rounded mx-auto d-block"  id ="imgsize"alt="nasa"></a>
        </div>
        </div>
        </div>
        </div>
    </div>

    {{/each}}
