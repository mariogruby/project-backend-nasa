<div>
        <p class="h4">{{news.date}}</p>
        <h1 class="card-title">{{news.title}}</h1>
        <div class="clearfix">
        <img src={{news.url}}  class="rounded float-left col-md-6  mb-3 ms-md-3 " id="img-id-config" alt="nasa">
        <p class="lead left col-md-6  mb-3 ms-md-3">{{news.explanation}}</p>
        </div>
    </div>

    {{#each Newscomments.result.0.comments}}
        <div>
           <p>User: <a href="/auth/user/{{author.id}}">{{author.username}}</a></p>
           <p>Date: {{date}}</p>
           <p>Message: {{contenido}}</p> 
        </div>
    {{/each}}
{{!-- <p>{{Newscomments.result.1.comments.0.contenido}}</p> --}}
<div>
    <form action="/news/{{news.date}}" method="post">
        <label for="">
            <input type="textarea" name="contenido">
        </label>

        <button type="submit">Enviar</button>
    </form>
</div>
